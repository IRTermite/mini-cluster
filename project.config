[project]
	description = NUC Mini Cluster
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Dale Bracey (IRTermite)
[access "refs/for/refs/*"]
	read = group user/Dale Bracey (IRTermite)
	push = group user/Dale Bracey (IRTermite)
	label-Code-Review = -2..+2 group user/Dale Bracey (IRTermite)
	label-Verified = -1..+1 group user/Dale Bracey (IRTermite)
	rebase = group user/Dale Bracey (IRTermite)
	removeReviewer = group user/Dale Bracey (IRTermite)
	submit = group user/Dale Bracey (IRTermite)
[access "refs/heads/*"]
	read = group user/Dale Bracey (IRTermite)
	create = group user/Dale Bracey (IRTermite)
	push = +force group user/Dale Bracey (IRTermite)
	pushMerge = group user/Dale Bracey (IRTermite)
[access "refs/tags/*"]
	push = +force group user/Dale Bracey (IRTermite)
	pushTag = group user/Dale Bracey (IRTermite)
	pushSignedTag = group user/Dale Bracey (IRTermite)
